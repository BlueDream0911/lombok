<project name="testDelombokAntTask" basedir="../../.." default="test">
	<taskdef classname="lombok.delombok.ant.DelombokTask" classpath="dist/lombok.jar" name="delombok" />
	<target name="test">
		<mkdir dir="build/testResult" />
		<delombok verbose="true" encoding="UTF-8" to="build/testResult">
			<fileset dir="test/lombok/resource" />
			<fileset dir="test/delombok/resource" />
		</delombok>
	</target>
	<target name="clean">
		<delete quiet="true" dir="build/testResult" />
	</target>
</project>
